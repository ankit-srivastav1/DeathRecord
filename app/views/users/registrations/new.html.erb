
<center>
<div class="page-wrapper bg-blue p-t-100 p-b-100 font-robo">
  <div class="wrapper wrapper--w680">
    <div class="card card-1">
      <div class="card-heading"></div>
      <div class="card-body">
        <h2 class="title">Registration Info</h2>
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>

        <div class="input-group">
          <%= f.label :first_name %> &nbsp &nbsp &nbsp  &nbsp  &nbsp  &nbsp  &nbsp  &nbsp  &nbsp  &nbsp
          <%= f.text_field:first_name, class: "input--style-1" %>
        </div>
 <br>


        <div class="input-group">
          <%= f.label :last_name %> &nbsp &nbsp &nbsp  &nbsp  &nbsp  &nbsp  &nbsp  &nbsp  &nbsp  &nbsp  &nbsp
          <%= f.text_field :last_name , class: "input--style-1"%>
        </div>
<br>

        <div class="input-group">
          <%= f.label :age%> &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp  &nbsp  &nbsp  &nbsp  &nbsp &nbsp   &nbsp  &nbsp  &nbsp  &nbsp  &nbsp
          <%= f.text_field :age,class: "input--style-1" %>
        </div>
<br>




        <div class="input-group">
          <%= f.label :email %>&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp  &nbsp  &nbsp &nbsp  &nbsp  &nbsp  &nbsp  &nbsp   &nbsp  &nbsp  &nbsp  &nbsp
          <%= f.email_field :email, class: "input--style-1" %>
        </div>

<br>
        <div class="input-group">
          <%= f.label :password %>
          <% if @minimum_password_length %>
          <em>(<%= @minimum_password_length %>characters)</em> &nbsp  &nbsp
          <% end %>
          <%= f.password_field :password, class: "input--style-1", autocomplete: "new-password" %>
        </div>
<br>

        <div class="input-group">
          <%= f.label :password_confirmation %> &nbsp  &nbsp
          <%= f.password_field :password_confirmation, class: "input--style-1",  autocomplete: "new-password" %>
        </div>


        <div class="p-t-20">
          <%= f.submit "Sign up" ,class: "btn btn--radius btn--green"  %>
        </div>
        <% end %>



        <%= render "users/shared/links" %>

        </div>
      </div>
    </div>
  </div>

</center>
